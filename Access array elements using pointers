#include<iostream>
using namespace std;
int main() 
{
    int arr[6];
    int *p;
    
    cout<<"Enter elements: ";
    for(int i=0;i<6;i++)
    {
        cin>>arr[i];
    }
    p=arr;
    cout<<"You entered: ";
    for(int i=0;i<6;i++)
    {
       std::cout<<*p<<endl;
        p++;
    }
    return 0;
}
